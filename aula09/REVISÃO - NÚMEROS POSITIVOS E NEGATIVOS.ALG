Algoritmo "semnome"

Var
num: inteiro

Inicio
escreval("Digite um número inteiro")
leia(num)

se (num > 0) entao
     escreval ("O número", num," é Positivo")
senao

se (num < 0) entao
     escreval ("O número", num," é Negativo")
senao

se (num = 0) entao
   escreval("O número",num," é Nêutro")

fimse
fimse
fimse

Fimalgoritmo


